{
  "compilerOptions": {
    "target": "es2022",
    "module": "ES2022",
    "outDir": "dist",
    "moduleResolution": "Node",
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true
  }
}